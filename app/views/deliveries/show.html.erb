<div class="page-header">
  <h3>Delivery #<%= @delivery.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @delivery.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/deliveries" class="btn btn-primary">
        Back
      </a>
      <a href="/deliveries/<%= @delivery.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_delivery/<%= @delivery.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
